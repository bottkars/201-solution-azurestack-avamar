{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "authenticationType": {
            "type": "string",
            "defaultValue": "password",
            "allowedValues": [
                "sshPublicKey",
                "password"
            ],
            "metadata": {
                "description": "Type of authentication to use on the Virtual Machine. SSH key is recommended."
            }
        },
        "AVECommonPassword": {
            "type": "securestring",
            "defaultValue": "Changeme123#",
            "metadata": {
                "description": "The password for AVE user."
            }
        },
        "AVEImageURI": {
            "type": "string",
            "defaultValue": "https://opsmanagerimage.blob.local.azurestack.external/images/AZURE-AVE-19.2.0.155.vhd",
            "metadata": {
                "description": "Local Uri of the Operations Manager VHD to create Custom image from"
            }
        },
        "AVEName": {
            "type": "string",
            "defaultValue": "aveazs1",
            "maxLength": 10,
            "metadata": {
                "description": "Name of the AVE virtual machine"
            }
        },
        "AVEPasswordOrKey": {
            "type": "securestring",
            "defaultValue": "Change_Me12345_",
            "metadata": {
                "description": "The password for AVE Settings."
            }
        },
        "AVESizeInTB": {
            "type": "string",
            "allowedValues": [
                "2",
                "4"
            ],
            "defaultValue": "2",
            "metadata": {
                "description": "The virtual machine size of the AVE in TB: 2TB is mapped to Standard_A5, and 4TB is mapped to Standard_A6."
            }
        },
        "AVEUsername": {
            "type": "string",
            "metadata": {
                "description": "The username for AVE."
            }
        },
        "AVEVhd": {
            "type": "string",
            "defaultValue": "AZURE-AVE-19.2.0.155.vhd",
            "metadata": {
                "description": "AVEn VHD Name"
            }

        },
        "diagnosticsStorageAccountExistingResourceGroup": {
            "type": "string",
            "defaultValue": "registry",
            "metadata": {
                "description": "The name of the Resource Group which contains the existing storage account to be used by diagnostics."
            }
        },
        "diagnosticsStorageAccountName": {
            "type": "string",
            "defaultValue": "ckboxhxdal63isalinuxvm",
            "metadata": {
                "description": "Storage account to be used by diagnostics. Diagnostics supports only Standard_GRS,Standard_LRS,Standard_RAGRS storage account types."
            }
        },
        "vnetExistingResourceGroup": {
            "type": "string",
            "defaultValue": "registry",
            "metadata": {
                "description": "The name of the Resource Group which contains the existing Virtual Network that AVE/DDVE will be connected to."
            }
        },
        "vnetName": {
            "type": "string",
            "defaultValue": "harborVNET",
            "metadata": {
                "description": "The name of the existing Virtual Network that AVE/DDVE will be connected to."
            }
        },
        "vnetSubnetName": {
            "type": "string",
            "defaultValue": "Subnet1",
            "metadata": {
                "description": "The name of the existing subnet that AVE/DDVE will be connected to."
            }
        }
    },
    "variables": {
        "AVEExternalHostname": "node",
        "apiVersion": "2017-06-01",
        "aveDataDisk": "[concat(parameters('AVEName'), '-datadisk')]",
        "aveDataDiskSize": 931,
        "aveNIC": "[concat(parameters('AVEName'), '-nic-0')]",
        "aveNsg": "[concat(parameters('AVEName'), '-nsg')]",
        "aveOSDisk": "[concat(parameters('AVEName'), '-osdisk')]",
        "aveVersion": "[replace(parameters('AVEVhd'),'.vhd', '')]",
        "aveVmSize": "[if(equals(parameters('AVESizeInTB'), '2'), 'Standard_A5', 'Standard_A6')]",
        "linuxConfiguration": {
            "disablePasswordAuthentication": true,
            "ssh": {
                "publicKeys": [
                    {
                        "path": "[concat('/home/', parameters('AVEUsername'), '/.ssh/authorized_keys')]",
                        "keyData": "[parameters('AVEPasswordOrKey')]"
                    }
                ]
            }
        },
        "numAveDataDisks": "[if(equals(parameters('AVESizeInTB'), '2'), 3, 6)]",
        "subnetId": "[concat(variables('vnetId'), '/subnets/', parameters('vnetSubnetName'))]",
        "vnetId": "[resourceId(parameters('vnetExistingResourceGroup'), 'Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
        "filesToDownload": [
            "https://raw.githubusercontent.com/bottkars/201-solution-azurestack-avamar/master/scripts/deploy_base.sh",
            "https://raw.githubusercontent.com/bottkars/201-solution-azurestack-avamar/master/scripts/configure-ave-azs.sh"
        ]
    },
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[concat(parameters('AVEName'),'/initvm')]",
            "apiVersion": "2017-03-30",
            "location": "[resourceGroup().location]",
            "properties": {
                "publisher": "Microsoft.Azure.Extensions",
                "type": "CustomScript",
                "typeHandlerVersion": "2.0",
                "autoUpgradeMinorVersion": true,
                "settings": {
                    "fileUris": "[variables('filesToDownload')]",
                    "timestamp": 2
                },
                "protectedSettings": {
                    "commandToExecute": "[concat('bash -l -c \"./deploy_base.sh 2>&1 | tee /home/', parameters('AVEUsername'), '/install.log\"')]"
                }
            },
            "dependsOn": [
                "[concat('Microsoft.Compute/virtualMachines/', parameters('AVEName'))]"
            ]
        },
        {
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[parameters('AVEName')]",
            "apiVersion": "2017-03-30",
            "location": "[resourceGroup().location]",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "[variables('aveVmSize')]"
                },
                "storageProfile": {
                    "imageReference": {
                        "id": "[resourceId('Microsoft.Compute/images', variables('AVEVersion'))]"
                    },
                    "osDisk": {
                        "name": "[variables('aveOSDisk')]",
                        "createOption": "FromImage",
                        "managedDisk": {
                            "storageAccountType": "Standard_LRS"
                        }
                    },
                    "copy": [
                        {
                            "name": "dataDisks",
                            "count": "[variables('numAveDataDisks')]",
                            "input": {
                                "caching": "None",
                                "diskSizeGB": "[variables('aveDataDiskSize')]",
                                "lun": "[copyIndex('dataDisks')]",
                                "name": "[concat(variables('aveDataDisk'), copyIndex('dataDisks'))]",
                                "managedDisk": {
                                    "storageAccountType": "Standard_LRS"
                                },
                                "createOption": "Empty"
                            }
                        }
                    ]
                },
                "osProfile": {
                    "computerName": "[parameters('AVEName')]",
                    "adminUsername": "[parameters('AVEUsername')]",
                    "adminPassword": "[parameters('AVEPasswordOrKey')]",
                    "customData": "[reference('Microsoft.Resources/deployments/generate-customdata', '2015-01-01').outputs.customData.value]",
                    "linuxConfiguration": "[if(equals(parameters('authenticationType'), 'password'), json('null'), variables('linuxConfiguration'))]"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('aveNIC'))]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "[reference(resourceId(parameters('diagnosticsStorageAccountExistingResourceGroup'), 'Microsoft.Storage/storageAccounts', parameters('diagnosticsStorageAccountName')), variables('apiVersion')).primaryEndpoints['blob']]"
                    }
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/networkInterfaces', variables('aveNIC'))]",
                "[concat('Microsoft.Compute/images/', variables('AVEVersion'))]"
            ]
        },
        {
